{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "IfAI",
  "version": "0.3.1",
  "identifier": "peterfei.IfAI",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "IfAI",
        "width": 1200,
        "height": 800,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    },
    "shell": {
      "open": true
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis",
      "msi"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon_256x256.png",
      "icons/Square128x128Logo.png",
      "icons/Square64x64Logo.png"
    ],
    "resources": [
      ".fastembed_cache/"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com",
      "nsis": {
        "installMode": "currentUser",
        "installerIcon": "icons/icon.ico",
        "license": "LICENSE",
        "languages": [
          "en-US",
          "zh-CN"
        ]
      }
    }
  }
}